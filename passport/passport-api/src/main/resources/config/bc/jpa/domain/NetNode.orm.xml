<?xml version="1.0" encoding="UTF-8"?>

<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_2_0.xsd" version="2.0">

    <named-query name="BC_READ_ALL_NET_NODE" >
        <query>SELECT netNode FROM cn.globalph.passport.domain.NetNode netNode</query>
    </named-query>
    
     <named-query name="NPH_NET_NODE_BY_COMMUNITY" >
        <query>SELECT nn FROM cn.globalph.passport.domain.NetNode nn 
        WHERE nn.community.id=:communityId</query>
    </named-query>
    
</entity-mappings>